@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>模型显示</title>
    <script src="../../Scripts/jquery.min.js" type="text/javascript"></script>
    <script src="../../Scripts/modelShow.js" type="text/javascript"></script>
</head>
<body>
    <!-- 工具条 starts -->
    @RenderPage("../mainForm/top.cshtml")
    <!-- 工具条 ends -->
    <div class="ch-container">
        <div class="row">
            <!-- 左菜单 starts -->
            @RenderPage("../mainForm/left.cshtml")
            <!-- 左菜单 ends -->
            <!-- 内容页 starts-->
            <div id="content" class="col-lg-10 col-sm-10">
                <div>
                    <ul class="mybreadcrumb">
                        <li><a href="#">系统组成</a></li>
                        <li><a href="/mainForm/modelShow">模型显示</a></li>
                    </ul>
                </div>
                <!-- 零件相册 starts-->
                <div class="row">
                    <div class="box col-md-12">
                        <div class="box-inner">
                            <div class="box-header well" data-original-title="">
                                <h2 class="h-blue">
                                    <i class="glyphicon glyphicon-edit"></i>模型3D影院</h2>
                                <div class="box-icon">
                                    <a href="#" id="prt_aMin" class="btn btn-minimize btn-round btn-default"><i id="prt_iMin"
                                        class="glyphicon glyphicon-chevron-up"></i></a>
                                </div>
                            </div>
                            <div id="prtDiv" style="display: block">
                                <div class="box-content">
                                    <div class="box-north">
                                        <form id="prtForm" action="" method="post">
                                        <label class="blue-label-se">
                                            系列</label><select class="sel-sm" name="serail" id="sel_serail">
                                                <option value="1" selected="selected">1</option>
                                                <option value="2">2</option>
                                            </select>
                                        <label class="blue-label-se">
                                            类型</label><select class="sel-sm" name="type" id="sel_type">
                                                <option value="1" selected="selected">I 型</option>
                                                <option value="2">Ⅱ型</option>
                                            </select>
                                        <label class="blue-label-se">
                                            模数</label><select class="sel-sm-moshu" name="moshu" id="sel_moshu">
                                                <option value="1" selected="selected">1</option>
                                                <option value="1.25">1.25</option>
                                                <option value="1.5">1.5</option>
                                                <option value="2">2</option>
                                                <option value="2.5">2.5</option>
                                                <option value="3">3</option>
                                                <option value="4">4</option>
                                                <option value="5">5</option>
                                                <option value="6">6</option>
                                                <option value="8">8</option>
                                                <option value="10">10</option>
                                            </select>
                                        <input id="selPrt" type="button" class="button-sm-se btn-primary" value="放映" />
                                        </form>
                                    </div>
                                    <div>
                                        <section id="dg-container" class="dg-container">
                                            <div class="dg-wrapper" id="prtAlbum">
                                                <a href="#" id="dynImg1" class="gallery-css-1">
                                                    <img src="../../Content/images/m1/m1-1-1_1.png" alt="齿轮滚刀">
                                                </a><a href="#" id="dynImg2" class="gallery-css-2">
                                                    <img src="../../Content/images/m1/m1-1-1_2.png" alt="齿轮滚刀">
                                                </a><a href="#" id="dynImg3" class="gallery-css-3">
                                                    <img src="../../Content/images/m1/m1-1-1_3.png" alt="齿轮滚刀">
                                                </a><a href="#" id="dynImg4" class="gallery-css-4">
                                                    <img src="../../Content/images/m1/m1-1-1_4.png" alt="齿轮滚刀">
                                                </a><a href="#" id="dynImg5" class="gallery-css-5">
                                                    <img src="../../Content/images/m1/m1-1-1_5.png" alt="齿轮滚刀">
                                                </a>
                                            </div>
                                            <nav>
                                                <span class="dg-prev">&lt;</span> <span class="dg-next">&gt;</span>
                                            </nav>
                                        </section>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- 零件相册 ends-->
            </div>
            <!-- 内容页 ends -->
        </div>
        <!-- 模态框 starts -->
        @RenderPage("../mainForm/modal.cshtml")
        <!-- 模态框 ends -->
    </div>
    <hr>
</body>
</html>
