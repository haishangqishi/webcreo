@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>齿轮滚刀系统</title>
    <script src="../../Scripts/jquery.min.js" type="text/javascript"></script>
    <script src="../../Scripts/ncfile.js" type="text/javascript"></script>
</head>
<body>
    <!-- 工具条 starts -->
    @RenderPage("top.cshtml")
    <!-- 工具条 ends -->
    <div class="ch-container">
        <div class="row">
            <!-- 左菜单 starts -->
            @RenderPage("left.cshtml")
            <!-- 左菜单 ends -->
            <!-- 内容页 starts-->
            <div id="content" class="col-lg-10 col-sm-10">
                <div>
                    <ul class="breadcrumb">
                        <li><a href="#">系统组成</a></li>
                        <li><a href="#">NC文件</a></li>
                    </ul>
                </div>
                <!-- 生成NC文件 starts-->
                <div class="row">
                    <div class="box col-md-12">
                        <div class="box-inner">
                            <div class="box-header well" data-original-title="">
                                <h2 class="h-blue">
                                    <i class="glyphicon glyphicon-edit"></i>数控工作室</h2>
                                <div class="box-icon">
                                    <a href="#" id="nc_aMin" class="btn btn-minimize btn-round btn-default"><i id="nc_iMin"
                                        class="glyphicon glyphicon-chevron-up"></i></a>
                                </div>
                            </div>
                            <div id="ncDiv" class="box-content" style="display: block">
                                <div class="box-content">
                                    <form id="ncForm" action="" method="post">
                                    <label class="blue-label-se">
                                        页容量</label><select class="sel-sm" name="pageSize"><option value="10" selected="selected">
                                            10</option>
                                            <option value="15">15</option>
                                            <option value="20">20</option>
                                        </select>
                                    <label class="blue-label-se">
                                        标准</label><input id="isStandard" name="isStandard" type="checkbox" checked="checked"
                                            class="check-sm-se" />
                                    <label class="blue-label-se">
                                        模数</label><input id="MN_sel_nc" name="moshu" type="text" class="input-sm-se" />
                                    <label class="blue-label-se">
                                        容屑槽数</label><input id="ZG_sel_nc" name="rongxieNum" type="text" class="input-sm-se" />
                                    <label class="blue-label-se">
                                        外径</label><input id="DEG_sel_nc" name="deg" type="text" class="input-sm-se" />
                                    <input id="selnc" type="button" class="button-sm-se btn-primary" value="查询" />
                                    </form>
                                    <form id="fileForm" action="/mainForm/generateFile" method="post">
                                    <input type="text" class="input-hid" id='paraId' name="paraId" value="" />
                                    <input type="submit" class="button-sm-file btn-primary" value="生成NC文件" />
                                    </form>
                                    <table class="table table-striped table-bordered responsive">
                                        <thead>
                                            <tr>
                                                <th>
                                                    选型
                                                </th>
                                                <th>
                                                    模数
                                                </th>
                                                <th>
                                                    容屑槽数
                                                </th>
                                                <th>
                                                    外径
                                                </th>
                                                <th>
                                                    全长
                                                </th>
                                                <th>
                                                    内孔直径
                                                </th>
                                                <th>
                                                    是否标准
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody id="ncbody">
                                            @*@foreach (var parm in ViewBag.list)
                                            { 
                                                <tr class="ta-tr">
                                                    <td>
                                                        <input type='radio' name='ra_@parm.parm_id' />
                                                    </td>
                                                    <td class="dyn">
                                                        @parm.moshu
                                                    </td>
                                                    <td>
                                                        @parm.rongxieNum
                                                    </td>
                                                    <td class="dyn">
                                                        @parm.deg
                                                    </td>
                                                    <td class="dyn">
                                                        @parm.L
                                                    </td>
                                                    <td class="dyn">
                                                        @parm.kongjing
                                                    </td>
                                                    <td>
                                                        <a class="btn btn-success btn-mid" href="#"><i class="glyphicon glyphicon-zoom-in icon-white i-padd">
                                                        </i>详细</a><a class="btn btn-danger btn-mid" href="#"><i class="glyphicon glyphicon-trash icon-white i-padd">
                                                        </i>删除</a>
                                                    </td>
                                                </tr>
                                            }*@
                                        </tbody>
                                    </table>
                                </div>
                                <div class="tcdPageCode">
                                    页码</div>
                                <script src="../../Scripts/pagiation.js" type="text/javascript"></script>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- 生成NC文件 ends-->
            </div>
            <!-- 内容页 ends -->
        </div>
        <!-- 模态框 starts -->
        @RenderPage("modal.cshtml")
        <!-- 模态框 ends -->
    </div>
    <hr>
</body>
</html>
