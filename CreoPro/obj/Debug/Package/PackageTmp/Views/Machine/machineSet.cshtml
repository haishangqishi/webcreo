@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>机床参数设置</title>
    <script src="../../Scripts/jquery.min.js" type="text/javascript"></script>
    <script src="../../Scripts/machineSet.js" type="text/javascript"></script>
</head>
<body>
    <!-- 工具条 starts -->
    @RenderPage("../mainForm/top.cshtml")
    <!-- 工具条 ends -->
    <div class="ch-container">
        <div class="row">
            <!-- 左菜单 starts -->
            @RenderPage("../mainForm/left.cshtml")
            <!-- 左菜单 ends -->
            <!-- 内容页 starts-->
            <div id="content" class="col-lg-10 col-sm-10">
                <div>
                    <ul class="mybreadcrumb">
                        <li><a href="#">机床管理</a></li>
                        <li><a href="/Machine/machineSet">机床参数设置</a></li>
                    </ul>
                </div>
                <!-- 工艺、机床列表 starts-->
                <div class="row">
                    <div class="box col-md-12">
                        <div class="box-inner">
                            <div class="box-header well" data-original-title="">
                                <h2 class="h-blue">
                                    <i class="glyphicon glyphicon-paperclip"></i>机床名片</h2>
                                <div class="box-icon">
                                    <a href="#" id="mdList_aMin" class="btn btn-minimize btn-round btn-default"><i id="mdList_iMin"
                                        class="glyphicon glyphicon-chevron-up"></i></a>
                                </div>
                            </div>
                            <div id="mdListDiv" class="box-content" style="display: block">
                                <div class="box-content1">
                                    <div class="img" style="float: right">
                                        <img alt="机床" src="@ViewBag.picPath">
                                    </div>
                                    <div class="resume-content">
                                        <div class="col">
                                            <div class="title display-table">
                                                <p>
                                                    机床名称
                                                </p>
                                                <span class="pull-right">@ViewBag.machName</span>
                                            </div>
                                            @{if (ViewBag.list1 != null)
                                              {
                                                  foreach (var machine in ViewBag.list1)
                                                  {
                                                <div class="title display-table">
                                                    <p>@machine.machPara
                                                    </p>
                                                    <span class="pull-right sp-br">@machine.machParaValue</span>
                                                </div>
                                                  }
                                              }
                                            }
                                        </div>
                                        <div class="col">
                                            @{if (ViewBag.list2 != null)
                                              {
                                                  foreach (var machine in ViewBag.list2)
                                                  {
                                                <div class="title display-table">
                                                    <p>@machine.machPara
                                                    </p>
                                                    <span class="pull-right sp-br">@machine.machParaValue</span>
                                                </div>
                                                  }
                                              }
                                            }
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- 工艺、机床列表 ends-->
            </div>
        </div>
        <!-- 内容页 ends -->
        <!-- 模态框 starts -->
        @RenderPage("../mainForm/modal.cshtml")
        <!-- 模态框 ends -->
    </div>
    <hr>
</body>
</html>
