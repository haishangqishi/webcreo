@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>齿轮滚刀系统</title>
    <script src="../../Scripts/jquery.min.js" type="text/javascript"></script>
    <script src="../../Scripts/sysHelp.js" type="text/javascript"></script>
</head>
<body>
    <!-- 工具条 starts -->
    @RenderPage("../mainForm/top.cshtml")
    <!-- 工具条 ends -->
    <div class="ch-container">
        <div class="row">
            <!-- 左菜单 starts -->
            @RenderPage("../mainForm/left.cshtml")
            <!-- 左菜单 ends -->
            <!-- 内容页 starts-->
            <div id="content" class="col-lg-10 col-sm-10">
                <div>
                    <ul class="breadcrumb">
                        <li><a href="#">基础信息</a></li>
                        <li><a href="#">系统助手</a></li>
                    </ul>
                </div>
                <div class="row">
                    <div class="box col-md-12">
                        <div class="box-inner">
                            <div class="box-header well" data-original-title="">
                                <h2 class="h-blue">
                                    <i class="glyphicon glyphicon-edit"></i>应用仓库</h2>
                                <div class="box-icon">
                                    <a href="#" id="exe_aMin" class="btn btn-minimize btn-round btn-default"><i id="exe_iMin"
                                        class="glyphicon glyphicon-chevron-up"></i></a>
                                </div>
                            </div>
                            <div id="exeDiv" class="box-content" style="height: 280px; display: block">
                                <div class="box-content">
                                    <form id="exeForm" action="/mainForm/downloadExe" method="post">
                                    <input type="submit" class="button-sm-file btn-primary" value="下载应用" />
                                    </form>
                                    <form id="regForm" action="/mainForm/downloadReg" method="post">
                                    <input type="submit" class="button-sm-file btn-primary" value="注册表" />
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- 内容页 ends -->
            <!-- 模态框 starts -->
            @RenderPage("../mainForm/modal.cshtml")
            <!-- 模态框 ends -->
        </div>
        <hr>
    </div>
</body>
</html>
