@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>齿轮滚刀系统</title>
    <script src="../../Scripts/jquery.min.js" type="text/javascript"></script>
    <script src="../../Scripts/pagiation.js" type="text/javascript"></script>
    <script src="../../Scripts/paraInput.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(".tcdPageCode").createPage({
            pageCount: 10,
            current: 1,
            backFn: function (p) {
                console.log(p);
            }
        });
    </script>
</head>
<body>
    <!-- 工具条 starts -->
    @RenderPage("top.cshtml")
    <!-- 工具条 ends -->
    <div class="ch-container">
        <div class="row">
            <!-- 左菜单 starts -->
            @RenderPage("left.cshtml")
            <!-- 左菜单 ends -->
            <!-- 内容页 starts-->
            <div id="content" class="col-lg-10 col-sm-10">
                <div>
                    <ul class="breadcrumb">
                        <li><a href="#">系统组成</a></li>
                        <li><a href="/mainForm/paraInput">参数输入</a></li>
                    </ul>
                </div>
                <!-- 探险入口 starts-->
                <div class="row">
                    <div class="box col-md-12">
                        <div class="box-inner">
                            <div class="box-header well" data-original-title="">
                                <h2 class="h-blue">
                                    <i class="glyphicon glyphicon-edit"></i>探险入口</h2>
                                <div class="box-icon">
                                    <a href="#" id="para_aMin" class="btn btn-minimize btn-round btn-default"><i id="para_iMin"
                                        class="glyphicon glyphicon-chevron-up"></i></a>
                                </div>
                            </div>
                            <div id="paraDiv" class="box-content" style="height: 290px; display: block">
                                <form id="paraForm" action="" method="post" role="form">
                                <div class="col-lg-10 col-sm-10">
                                    <div class="form-group has-success col-md-2">
                                        <label class="blue-label" for="mushu">
                                            模数</label>
                                        <input type="text" class="form-control blue-input" id="mushu" name="MN">
                                    </div>
                                    <div class="form-group has-success col-md-2">
                                        <label class="blue-label" for="rongxieNum">
                                            容屑槽数</label>
                                        <input type="text" class="form-control blue-input" id="rongxieNum" name="ZG">
                                    </div>
                                    <div class="form-group has-success col-md-2">
                                        <label class="blue-label" for="waijing">
                                            外径</label>
                                        <input type="text" class="form-control blue-input" id="waijing" name="DEG">
                                    </div>
                                    <div class="form-group has-success col-md-2">
                                        <label class="blue-label" for="quanchang">
                                            全长</label>
                                        <input type="text" class="form-control blue-input" id="quanchang" name="L">
                                    </div>
                                </div>
                                <div class="col-lg-10 col-sm-10">
                                    <div class="form-group has-success col-md-2">
                                        <label class="blue-label" for="zhoutaiD">
                                            轴台直径</label>
                                        <input type="text" class="form-control blue-input" id="zhoutaiD" name="DL">
                                    </div>
                                    <div class="form-group has-success col-md-2">
                                        <label class="blue-label" for="zhoutaiL">
                                            轴台长度</label>
                                        <input type="text" class="form-control blue-input" id="zhoutaiL" name="A">
                                    </div>
                                    <div class="form-group has-success col-md-2">
                                        <label class="blue-label" for="neikongD">
                                            内孔直径</label>
                                        <input type="text" class="form-control blue-input" id="neikongD" name="D">
                                    </div>
                                    <div class="form-group has-success col-md-2">
                                        <label class="blue-label" for="kongdaoD">
                                            内孔空刀直径</label>
                                        <input type="text" class="form-control blue-input" id="kongdaoD" name="DLO">
                                    </div>
                                </div>
                                <div class="col-lg-10 col-sm-10">
                                    <div class="form-group has-success col-md-2">
                                        <label class="blue-label" for="kongdaoL">
                                            内孔空刀长度</label>
                                        <input type="text" class="form-control blue-input" id="kongdaoL" name="L1">
                                    </div>
                                    <div class="form-group has-success col-md-2">
                                        <label class="blue-label" for="jiancaoW">
                                            键槽宽度</label>
                                        <input type="text" class="form-control blue-input" id="jiancaoW" name="B">
                                    </div>
                                    <div class="form-group has-success col-md-2">
                                        <label class="blue-label" for="jiancaoW">
                                            键槽高度</label>
                                        <input type="text" class="form-control blue-input" id="jiancaoH" name="T1">
                                    </div>
                                    <div class="form-group has-success col-md-2">
                                        <label class="blue-label" for="daojiaoL">
                                            倒角边长</label>
                                        <input type="text" class="form-control blue-input" id="daojiaoL" name="C">
                                    </div>
                                </div>
                                <div class="col-lg-10 col-sm-10">
                                    <div class="form-group has-success col-md-5">
                                    </div>
                                    <button id="startCreo" class="btn btn-primary btn-start">
                                        启动Creo</button>
                                    <button id="createModel" class="btn btn-primary btn-start">
                                        生成模型</button>
                                    <button id="test" class="btn btn-primary btn-start">
                                        测试</button>
                                </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- 探险入口 ends-->
                <!-- 参数基地 starts-->
                <div class="row">
                    <div class="box col-md-12">
                        <div class="box-inner">
                            <div class="box-header well" data-original-title="">
                                <h2 class="h-blue">
                                    <i class="glyphicon glyphicon-edit"></i>参数基地</h2>
                                <div class="box-icon">
                                    <a href="#" id="paralist_aMin" class="btn btn-minimize btn-round btn-default"><i
                                        id="paralist_iMin" class="glyphicon glyphicon-chevron-up"></i></a>
                                </div>
                            </div>
                            <div id="paralistDiv" class="box-content" style="height: 290px; display: block">
                                <table class="table table-striped table-bordered responsive">
                                    <thead>
                                        <tr>
                                            <th>
                                                模数
                                            </th>
                                            <th>
                                                容屑槽数
                                            </th>
                                            <th>
                                                外径
                                            </th>
                                            <th>
                                                全长
                                            </th>
                                            <th>
                                                内孔直径
                                            </th>
                                            <th>
                                                操作
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var parm in ViewBag.list)
                                        { 
                                            <tr>
                                                <td class="dyn">
                                                    @parm.moshu
                                                </td>
                                                <td>
                                                    @parm.rongxieNum
                                                </td>
                                                <td class="dyn">
                                                    @parm.deg
                                                </td>
                                                <td class="dyn">
                                                    @parm.L
                                                </td>
                                                <td class="dyn">
                                                    @parm.kongjing
                                                </td>
                                                <td>
                                                    <a class="btn btn-success btn-mid" href="#"><i class="glyphicon glyphicon-zoom-in icon-white i-padd">
                                                    </i>查看</a><a class="btn btn-info btn-mid" href="#"><i class="glyphicon glyphicon-edit icon-white i-padd">
                                                    </i>编辑</a><a class="btn btn-danger btn-mid" href="#"><i class="glyphicon glyphicon-trash icon-white i-padd">
                                                    </i>删除</a>
                                                </td>
                                            </tr>
                                        }
                                        @*<tr>
                                            <td>
                                                Abraham
                                            </td>
                                            <td>
                                                2012/03/01
                                            </td>
                                            <td>
                                                Member
                                            </td>
                                            <td>
                                                Member
                                            </td>
                                            <td>
                                                Member
                                            </td>
                                            <td>
                                                <a class="btn btn-success btn-mid" href="#"><i class="glyphicon glyphicon-zoom-in icon-white i-padd">
                                                </i>查看</a><a class="btn btn-info btn-mid" href="#"><i class="glyphicon glyphicon-edit icon-white i-padd">
                                                </i>编辑</a><a class="btn btn-danger btn-mid" href="#"><i class="glyphicon glyphicon-trash icon-white i-padd">
                                                </i>删除</a>
                                            </td>
                                        </tr>*@
                                    </tbody>
                                </table>
                            </div>
                            <div class="tcdPageCode">
                            </div>
                        </div>
                    </div>
                </div>
                <!-- 参数基地 ends-->
            </div>
        </div>
        <!-- 内容页 ends -->
        <!-- 模态框 starts -->
        @RenderPage("modal.cshtml")
        <!-- 模态框 ends -->
    </div>
    <hr>
</body>
</html>
