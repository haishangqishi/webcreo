@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>齿轮滚刀系统</title>
    <script src="../../Scripts/jquery.min.js" type="text/javascript"></script>
    <script src="../../Scripts/pagiation.js" type="text/javascript"></script>
    <script src="../../Scripts/paraInput.js" type="text/javascript"></script>
</head>
<body>
    <!-- 工具条 starts -->
    @RenderPage("top.cshtml")
    <!-- 工具条 ends -->
    <div class="ch-container">
        <div class="row">
            <!-- 左菜单 starts -->
            @RenderPage("left.cshtml")
            <!-- 左菜单 ends -->
            <!-- 内容页 starts-->
            <div id="content" class="col-lg-10 col-sm-10">
                <div>
                    <ul class="mybreadcrumb">
                        <li><a href="#">系统组成</a></li>
                        <li><a href="/mainForm/paraInput">参数输入</a></li>
                    </ul>
                </div>
                <!-- 探险入口 starts-->
                <div class="row">
                    <div class="box col-md-12">
                        <div class="box-inner">
                            <div class="box-header well" data-original-title="">
                                <h2 class="h-blue">
                                    <i class="glyphicon glyphicon-edit"></i>探险入口</h2>
                                <div class="box-icon">
                                    <a href="#" id="para_aMin" class="btn btn-minimize btn-round btn-default"><i id="para_iMin"
                                        class="glyphicon glyphicon-chevron-up"></i></a>
                                </div>
                            </div>
                            <div id="paraDiv" class="box-content" style="height: 145px; display: block">
                                <form id="paraForm" action="" method="post" role="form">
                                <div class="col-lg-10 col-sm-10">
                                    <label class="blue-label-pa">
                                        模数</label><input type="text" class="input-sm-pa" id="mushu" name="MN" />
                                    <label class="blue-label-pa">
                                        容屑槽数</label><input type="text" class="input-sm-pa" id="rongxieNum" name="ZG" />
                                    <label class="blue-label-pa">
                                        外径</label><input type="text" class="input-sm-pa" id="waijing" name="DEG" />
                                    <label class="blue-label-pa">
                                        全长</label><input type="text" class="input-sm-pa" id="quanchang" name="L" />
                                </div>
                                <div class="col-lg-10 col-sm-10">
                                    <label class="blue-label-pa">
                                        轴台直径</label><input type="text" class="input-sm-pa" id="zhoutaiD" name="DL" />
                                    <label class="blue-label-pa">
                                        轴台长度</label><input type="text" class="input-sm-pa" id="zhoutaiL" name="A" />
                                    <label class="blue-label-pa">
                                        内孔直径</label><input type="text" class="input-sm-pa" id="neikongD" name="D" />
                                    <label class="blue-label-pa" title="内孔空刀直径">
                                        空刀直径</label><input type="text" class="input-sm-pa" id="kongdaoD" name="DLO" />
                                </div>
                                <div class="col-lg-10 col-sm-10">
                                    <label class="blue-label-pa" title="内孔空刀长度">
                                        空刀长度</label><input type="text" class="input-sm-pa" id="kongdaoL" name="L1" />
                                    <label class="blue-label-pa">
                                        键槽宽度</label><input type="text" class="input-sm-pa" id="jiancaoW" name="B" />
                                    <label class="blue-label-pa">
                                        键槽高度</label><input type="text" class="input-sm-pa" id="jiancaoH" name="T1" />
                                    <label class="blue-label-pa" title="内孔空刀直径">
                                        倒角边长</label><input type="text" class="input-sm-pa" id="daojiaoL" name="C" />
                                </div>
                                <div class="col-lg-10 col-sm-10">
                                    <div class="form-group my-col-md-5">
                                    </div>
                                    <button id="startCreo" class="btn btn-primary btn-create">
                                        快速选型</button>
                                    <button id="createModel" class="btn btn-primary btn-create">
                                        生成模型</button>
                                </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- 探险入口 ends-->
                <!-- 参数基地 starts-->
                <div class="row">
                    <div class="box col-md-12">
                        <div class="box-inner">
                            <div class="box-header well" data-original-title="">
                                <h2 class="h-blue">
                                    <i class="glyphicon glyphicon-edit"></i>参数基地</h2>
                                <div class="box-icon">
                                    <a href="#" id="paralist_aMin" class="btn btn-minimize btn-round btn-default"><i
                                        id="paralist_iMin" class="glyphicon glyphicon-chevron-up"></i></a>
                                </div>
                            </div>
                            <div id="paralistDiv" class="box-content" style="height: 320px; display: block">
                                <form id="paraSelectForm" action="" method="post" role="form">
                                <label class="blue-label-se">
                                    页容量</label><select class="sel-sm" name="pageSize"><option value="10" selected="selected">
                                        10</option>
                                        <option value="15">15</option>
                                        <option value="20">20</option>
                                    </select>
                                <label class="blue-label-se">
                                    模数</label><input id="MN_select" name="mushu" type="text" class="input-sm-se" />
                                <label class="blue-label-se">
                                    容屑槽数</label><input id="ZG_select" name="rongxieNum" type="text" class="input-sm-se" />
                                <label class="blue-label-se">
                                    外径</label><input id="DEG_select" name="deg" type="text" class="input-sm-se" />
                                <input id="selFilter" type="button" class="button-sm-se btn-primary" value="查询" />
                                </form>
                                @*可查看零件，重生是另存零件*@
                                <table class="table table-striped table-bordered responsive">
                                    <thead>
                                        <tr>
                                            <th>
                                                模数
                                            </th>
                                            <th>
                                                容屑槽数
                                            </th>
                                            <th>
                                                外径
                                            </th>
                                            <th>
                                                全长
                                            </th>
                                            <th>
                                                内孔直径
                                            </th>
                                            <th>
                                                操作
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody id="listbody">
                                        @foreach (var parm in ViewBag.list)
                                        { 
                                            <tr class="ta-tr">
                                                <td class="dyn">
                                                    @parm.moshu
                                                </td>
                                                <td>
                                                    @parm.rongxieNum
                                                </td>
                                                <td class="dyn">
                                                    @parm.deg
                                                </td>
                                                <td class="dyn">
                                                    @parm.L
                                                </td>
                                                <td class="dyn">
                                                    @parm.kongjing
                                                </td>
                                                <td>
                                                    <a class="btn btn-success btn-mid" href="#"><i class="glyphicon glyphicon-zoom-in icon-white i-padd">
                                                    </i>查看</a><a class="btn btn-danger btn-mid" href="#"><i class="glyphicon glyphicon-trash icon-white i-padd">
                                                    </i>删除</a>
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                            <div class="tcdPageCode">
                                页码</div>
                        </div>
                    </div>
                </div>
                <!-- 参数基地 ends-->
            </div>
        </div>
        <!-- 内容页 ends -->
        <!-- 模态框 starts -->
        @RenderPage("modal.cshtml")
        <!-- 模态框 ends -->
    </div>
    <hr>
</body>
</html>
