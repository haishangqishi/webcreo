@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>刀具类型</title>
    <script src="../../Scripts/jquery.min.js" type="text/javascript"></script>
    <script src="../../Scripts/toolType.js" type="text/javascript"></script>
</head>
<body>
    <!-- 工具条 starts -->
    @RenderPage("../mainForm/top.cshtml")
    <!-- 工具条 ends -->
    <div class="ch-container">
        <div class="row">
            <!-- 左菜单 starts -->
            @RenderPage("../mainForm/left.cshtml")
            <!-- 左菜单 ends -->
            <!-- 内容页 starts-->
            <div id="content" class="col-lg-10 col-sm-10">
                <div>
                    <ul class="mybreadcrumb">
                        <li><a href="#">机床管理</a></li>
                        <li><a href="/Tool/toolType">刀具类型</a></li>
                    </ul>
                </div>
                <!-- 工艺、机床列表 starts-->
                <div class="row">
                    <div class="box col-md-8">
                        <div class="box-inner">
                            <div class="box-header well" data-original-title="">
                                <h2 class="h-blue">
                                    <i class="glyphicon glyphicon-edit"></i>刀具供应商</h2>
                                <div class="box-icon">
                                    <a href="#" id="toolList_aMin" class="btn btn-minimize btn-round btn-default"><i
                                        id="toolList_iMin" class="glyphicon glyphicon-chevron-up"></i></a>
                                </div>
                            </div>
                            <div id="toolListDiv" style="height: 310px; display: block">
                                <div class="box-content">
                                    <form id="toolSelectForm" action="" method="post" role="form">
                                    <label class="blue-label-se">
                                        工艺</label><input id="machProc_select" name="mushu" type="text" class="input-sm-se" />
                                    <input id="selTool" type="button" class="button-sm-se btn-primary" value="查询" />
                                    </form>
                                    <table class="table table-striped table-bordered responsive">
                                        <thead>
                                            <tr>
                                                <th>
                                                    工艺
                                                </th>
                                                <th>
                                                    刀具类型
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody id="machListBody">
                                            <tr>
                                                <td>
                                                    粗车外圆
                                                </td>
                                                <td>
                                                    <a href="/Tool/toolSet" class="">车刀</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    钻内孔
                                                </td>
                                                <td>
                                                    <a href="/Tool/toolSet" class="">钻头</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    精车外圆
                                                </td>
                                                <td>
                                                    <a href="/Tool/toolSet" class="">车刀</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    插键槽
                                                </td>
                                                <td>
                                                    <a href="/Tool/toolSet" class="">插齿刀</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    铣容屑槽
                                                </td>
                                                <td>
                                                    <a href="/Tool/toolSet" class="">铣刀</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    铲齿
                                                </td>
                                                <td>
                                                    <a href="/Tool/toolSet" class="">成形车刀</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    磨内孔
                                                </td>
                                                <td>
                                                    <a href="/Tool/toolSet" class="">砂轮</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    磨两端面
                                                </td>
                                                <td>
                                                    <a href="/Tool/toolSet" class="">砂轮</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    铲磨
                                                </td>
                                                <td>
                                                    <a href="/Tool/toolSet" class="">盘型砂轮</a>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- 工艺、机床列表 ends-->
            </div>
        </div>
        <!-- 内容页 ends -->
        <!-- 模态框 starts -->
        @RenderPage("../mainForm/modal.cshtml")
        <!-- 模态框 ends -->
    </div>
    <hr>
</body>
</html>
