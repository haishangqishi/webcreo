@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>刀具参数设置</title>
</head>
<body>
    <!-- 工具条 starts -->
    @RenderPage("../mainForm/top.cshtml")
    <!-- 工具条 ends -->
    <div class="ch-container">
        <div class="row">
            <!-- 左菜单 starts -->
            @RenderPage("../mainForm/left.cshtml")
            <!-- 左菜单 ends -->
            <!-- 内容页 starts-->
            <div id="content" class="col-lg-10 col-sm-10">
                <div>
                    <ul class="mybreadcrumb">
                        <li><a href="#">刀具管理</a></li>
                        <li><a href="/Tool/toolSet">刀具参数设置</a></li>
                    </ul>
                </div>
                <!-- 工艺、刀具列表 starts-->
                <div class="row">
                    <div class="box col-md-12">
                        <div class="box-inner">
