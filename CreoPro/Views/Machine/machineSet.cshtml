            <!-- 内容页 starts-->
            <div id="content" class="col-lg-10 col-sm-10">
                <div>
                    <ul class="mybreadcrumb">
                        <li><a href="#">机床管理</a></li>
                        <li><a href="/Machine/machineSet">机床参数设置</a></li>
                    </ul>
                </div>
                <!-- 工艺、机床列表 starts-->
                <div class="row">
                    <div class="box col-md-12">
                        <div class="box-inner">
                            <div class="box-header well" data-original-title="">
                                <h2 class="h-blue">
                                    <i class="glyphicon glyphicon-paperclip"></i>机床名片</h2>
                                <div class="box-icon">
                                    <a href="#" class="btn btn-minimize btn-round btn-default"><i class="glyphicon glyphicon-chevron-up">
                                    </i></a>
                                </div>
                            </div>
                            <div class="box-content" style="height: 476px; display: block">
                                <div class="box-content">
                                    <div class="img" style="float: right">
                                        <img alt="机床" src="@ViewBag.picPath">
                                    </div>
                                    <div class="resume-content">
                                        <div class="col">
                                            <div class="title display-table">
                                                <p>
                                                    机床名称
                                                </p>
                                                <span class="pull-right">@ViewBag.machName</span>
                                            </div>
                                            @foreach (var machine in ViewBag.list1)
                                            {
                                                <div class="title display-table">
                                                    <p>@machine.machPara
                                                    </p>
                                                    <span class="pull-right">@machine.machParaValue</span>
                                                </div>
                                            }
                                        </div>
                                        <div class="col">
                                            @foreach (var machine in ViewBag.list2)
                                            {
                                                <div class="title display-table">
                                                    <p>@machine.machPara
                                                    </p>
                                                    <span class="pull-right">@machine.machParaValue</span>
                                                </div>
                                            }
                                        </div>
