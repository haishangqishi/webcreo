                                        <strong>机床重量：</strong>约5T<br>*@
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- 工艺、机床列表 ends-->
            </div>
        </div>
        <!-- 内容页 ends -->
        <!-- 模态框 starts -->
        @RenderPage("../mainForm/modal.cshtml")
        <!-- 模态框 ends -->
    </div>
    <hr>
</body>
</html>
