                                                <td>
                                                    粗车外圆
                                                </td>
                                                <td>
                                                    <a href="/Machine/machineSet" class="">车床</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    钻内孔
                                                </td>
                                                <td>
                                                    <a href="/Machine/machineSet" class="">钻床</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    精车外圆
                                                </td>
                                                <td>
                                                    <a href="/Machine/machineSet" class="">车床</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    插键槽
                                                </td>
                                                <td>
                                                    <a href="/Machine/machineSet" class="">车床</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    铣容屑槽
                                                </td>
                                                <td>
                                                    <a href="/Machine/machineSet" class="">车床</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    铲齿
                                                </td>
                                                <td>
                                                    <a href="/Machine/machineSet" class="">铲齿机</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    磨内孔
                                                </td>
                                                <td>
                                                    <a href="/Machine/machineSet" class="">磨床</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    磨两端面
                                                </td>
                                                <td>
                                                    <a href="/Machine/machineSet" class="">磨床</a>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    铲磨
                                                </td>
                                                <td>
                                                    <a href="/Machine/machineSet" class="">数控铲磨床</a>
                                                </td>
                                            </tr>*@
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- 工艺、机床列表 ends-->
            </div>
        </div>
        <!-- 内容页 ends -->
        <!-- 模态框 starts -->
        @RenderPage("../mainForm/modal.cshtml")
        <!-- 模态框 ends -->
    </div>
    <hr>
</body>
</html>
