@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>机床类型</title>
    <script src="../../Scripts/jquery.min.js" type="text/javascript"></script>
    <script src="../../Scripts/machineType.js" type="text/javascript"></script>
</head>
<body>
    <!-- 工具条 starts -->
    @RenderPage("../mainForm/top.cshtml")
    <!-- 工具条 ends -->
    <div class="ch-container">
        <div class="row">
            <!-- 左菜单 starts -->
            @RenderPage("../mainForm/left.cshtml")
            <!-- 左菜单 ends -->
            <!-- 内容页 starts-->
            <div id="content" class="col-lg-10 col-sm-10">
                <div>
                    <ul class="mybreadcrumb">
                        <li><a href="#">机床管理</a></li>
                        <li><a href="/Machine/machineType">机床类型</a></li>
                    </ul>
                </div>
                <!-- 工艺、机床列表 starts-->
                <div class="row">
                    <div class="box col-md-12">
                        <div class="box-inner">
                            <div class="box-header well" data-original-title="">
                                <h2 class="h-blue">
                                    <i class="glyphicon glyphicon-edit"></i>机床供应商</h2>
                                <div class="box-icon">
                                    <a href="#" id="machList_aMin" class="btn btn-minimize btn-round btn-default"><i
                                        id="machList_iMin" class="glyphicon glyphicon-chevron-up"></i></a>
                                </div>
                            </div>
                            <div id="machListDiv" style="height: 330px; display: block">
                                <div class="box-content">
                                    <form id="machSelectForm" action="" method="post" role="form">
                                    <label class="blue-label-se">
                                        工艺</label><input id="machProc_select" name="mushu" type="text" class="input-sm-se" />
                                    <input id="selMach" type="button" class="button-sm-se btn-primary" value="查询" />
                                    </form>
                                    <table class="table table-striped table-bordered responsive">
                                        <thead>
                                            <tr>
                                                <th>
                                                    工艺
                                                </th>
                                                <th>
                                                    机床类型
                                                </th>
                                            </tr>
                                        </thead>
                                        <tbody id="machListBody">
                                            @foreach (var procMach in ViewBag.list)
                                            {
                                                <tr>
                                                    <td>
                                                        @procMach.procName
                                                    </td>
                                                    <td>
                                                        <form action="/Machine/getMachineSet" method="post" class="hi-form">
                                                        <a href="" class="td-mach">@procMach.machName</a>
                                                        <input name="txtMachName" type="text" class="hidden" value="@procMach.machName" />
                                                        <input name="txtMachId" type="text" class="hidden" value="@procMach.machId" />
                                                        </form>
                                                    </td>
                                                </tr>
                                            }
